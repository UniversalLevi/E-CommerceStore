{
  "name": "eazyds-backend",
  "version": "1.0.0",
  "description": "Backend API for EazyDS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed": "ts-node src/scripts/seedProducts.ts",
    "seed:niches": "ts-node src/scripts/seedNiches.ts",
    "seed:templates": "ts-node src/scripts/seedTemplates.ts",
    "migrate:niches": "ts-node src/scripts/assignNichesToProducts.ts",
    "migrate:analytics": "ts-node src/scripts/migrateProductAnalytics.ts",
    "create-admin": "ts-node src/scripts/createAdmin.ts",
